<?xml version="1.0" encoding="UTF-8"?>
<Renderer>
	<StageList>

		<Stage Name="S1" Type="normal" Enable="True">
			<Camera Name="default" />
			<ViewPort Name="default" TopLeftX="0" TopLeftY="0" Width="-1" Height="-1" MinDepth="0" MaxDepth="1.0" />
			<RenderTarget Type="backbuffer" Width="1.0" Height="1.0" />
		</Stage>

		<Stage Name="S2" Type="normal" Enable="true">
			<Camera Name="default" />
			<ViewPort Name="default" TopLeftX="0" TopLeftY="0" Width="-1" Height="-1" MinDepth="0" MaxDepth="1.0" />
			<RenderTarget Type="backbuffer" Width="1.0" Height="1.0" IsClearColor="False" IsClearDepth="False" IsClearStencil="False" />

			<Layer EntityType="Mirror" Transparent="No"> 
				<Blend>
					<RGB Src="One" Dest="Zero" Oper="Add" />
					<Alpha Src="One" Dest="Zero" Oper="Add" />
				</Blend>

				<Stencil>
					<DepthBuffer Enable="True" WriteMask="Zero" Func="Less"/>
					<StencilBuffer Enable="True" ReadMask="0xff" WriteMask="0xff"/>
					<Front Failed="Keep" DepthFailed="Keep" Pass="Keep" Func="Equal" />
					<Back Failed="Keep" DepthFailed="Keep" Pass="Keep" Func="Equal" />
				</Stencil>
			</Layer>

		</Stage>

	</StageList>
</Renderer>